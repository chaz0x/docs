        "groups": [
          {
            "group": "Account/User",
            "pages": [
              "api-reference/account-user/query_balance"
            ]
          },
          {
            "group": "Funds",
            "pages": [
              "api-reference/funds/deposit",
              "api-reference/funds/deposit_status",
              "api-reference/funds/withdraw",
              "api-reference/funds/withdraw_status"
            ]
          },
          {
